{
	"name": "@lenne.tech/multer-gridfs-storage",
	"version": "5.0.8",
	"description": "Multer storage engine for GridFS",
	"publishConfig": {
		"access": "public"
	},
	"main": "lib/index.js",
	"scripts": {
		"build": "npm run compile",
		"compile": "del-cli ./lib/** && tsc --declaration",
		"watch": "del-cli ./lib/** && tsc --declaration -w",
		"test": "npm run compile && (timeout 90s ava || true) && npm run test:cleanup",
		"test:cleanup": "ts-node test/global-cleanup.ts",
		"lint": "eslint {src,test}/**/*.ts --fix --cache",
		"coverage": "npm run compile && nyc --reporter=lcov ava --timeout=30s && npm run test:cleanup",
		"docs": "jsdoc -c ./conf.json --readme ./README.md",
		"coveralls": "npm run coverage | coveralls"
	},
	"xo": {
		"prettier": true,
		"rules": {
			"@typescript-eslint/restrict-plus-operands": 0
		}
	},
	"keywords": [
		"multer",
		"mongodb",
		"storage",
		"gridfs"
	],
	"author": {
		"name": "lenne.Tech",
		"email": "info@lenne.tech",
		"url": "https://github.com/lenneTech"
	},
	"contributors": [
		{
			"name": "devconcept",
			"email": "devconcept@outlook.com"
		},
		{
			"name": "theBGuy",
			"url": "https://github.com/theBGuy"
		},
		{
			"name": "lenne.Tech",
			"email": "info@lenne.tech",
			"url": "https://github.com/lenneTech"
		}
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lenneTech/multer-gridfs-storage"
	},
	"dependencies": {
		"has-own-prop": ">=2.0.0",
		"is-generator": "^1.0.3",
		"is-promise": "^4.0.0",
		"lodash.isplainobject": ">=0.8.0",
		"mongodb": ">=2",
		"mongodb-uri": "^0.9.7",
		"pump": "^3.0.0"
	},
	"peerDependencies": {
		"multer": ">=1.4.5-lts.1"
	},
	"devDependencies": {
		"@ava/typescript": "^6.0.0",
		"@eslint/js": "^9.31.0",
		"@types/express": "^5.0.3",
		"@types/multer": "^2.0.0",
		"@types/pump": "^1.1.3",
		"@types/sinon": "^17.0.4",
		"ava": "^6.4.1",
		"crypto-random-string": "^5.0.0",
		"del-cli": "^6.0.0",
		"delay": "^6.0.0",
		"eslint": "^9.31.0",
		"express": "^5.1.0",
		"globals": "^16.3.0",
		"jsdoc": "^4.0.4",
		"md5-file": "^5.0.0",
		"mongoose": "^8.16.4",
		"nyc": "^17.1.0",
		"pify": "^6.1.0",
		"sinon": "^21.0.0",
		"supertest": "^7.1.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.37.0"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"bugs": {
		"url": "https://github.com/lenneTech/multer-gridfs-storage/issues",
		"email": "info@lenne.tech"
	},
	"homepage": "https://github.com/lenneTech/multer-gridfs-storage",
	"files": [
		"LICENSE",
		"CHANGELOG.md",
		"README.md",
		"lib/"
	]
}
